{
  "name": "addresses-microservice",
  "version": "1.0.0",
  "description": "A microservice for managing addresses and locations",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "postinstall": "npm rebuild better-sqlite3"
  },
  "keywords": [
    "microservice",
    "addresses",
    "locations",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "helmet": "8.1.0",
    "knex": "3.0.1",
    "tsx": "4.21.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.21",
    "@types/node": "20.10.5",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "eslint": "8.56.0",
    "prettier": "3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "vitest": "1.0.4"
  }
}
